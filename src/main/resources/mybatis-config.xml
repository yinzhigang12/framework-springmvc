<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="com.mysql.jdbc.Driver" />
				<property name="url"	value="jdbc:mysql://localhost:3306/ajia_store?characterEncoding=UTF-8" />
				<property name="username" value="root" />
				<property name="password" value="" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper resource="com/ajiatech/mapper/AjiaCartItemMapper.xml"/>
		<mapper resource="com/ajiatech/mapper/AjiaCollectItemMapper.xml"/>
		<mapper resource="com/ajiatech/mapper/AjiaContentCategoryMapper.xml"/>
		<mapper resource="com/ajiatech/mapper/AjiaContentMapper.xml"/>
		<mapper resource="com/ajiatech/mapper/AjiaItemCatMapper.xml"/>
		<mapper resource="com/ajiatech/mapper/AjiaItemDescMapper.xml"/>
		<mapper resource="com/ajiatech/mapper/AjiaItemMapper.xml"/>
		<mapper resource="com/ajiatech/mapper/AjiaItemParamItemMapper.xml"/>
		<mapper resource="com/ajiatech/mapper/AjiaItemParamMapper.xml"/>
		<mapper resource="com/ajiatech/mapper/AjiaOrderItemMapper.xml"/>
		<mapper resource="com/ajiatech/mapper/AjiaOrderMapper.xml"/>
		<mapper resource="com/ajiatech/mapper/AjiaShippingMapper.xml" />
		<mapper resource="com/ajiatech/mapper/AjiaUserMapper.xml"/>
	</mappers>
</configuration>